{
  "name": "vscode-breakline-highlight",
  "displayName": "Breakline Highlight",
  "description": "Selection highlight for breaklines",
  "version": "1.1.0",
  "publisher": "pit00",
  "engines": { "vscode": "^1.62.0" },
  "categories": ["Other"],
  "keywords": ["whitespace","tab","space","show","gradient","selection"],
  "activationEvents": ["onStartupFinished"],
  "license": "MIT",
  "main": "./breaklineHighlight",
  "browser": "./breaklineHighlight",
  "icon": "assets/icon.png",
  "contributes": {
    "configuration": {
      "title": "breaklineHighlight",
      "properties": {
        "breaklineHighlight.color": {
          "type": "string",
          "default": "#DC143CB3",
          "description": "Color of char"
        },
        "breaklineHighlight.lf.text": {
            "type": "string",
            "default": "⇩",
            "description": "Text of LF char"
        },
        "breaklineHighlight.crlf.text": {
            "type": "string",
            "default": "⏎",
            "description": "Text of CRLF chars"
        }
      }
    }
  },
  "repository": {
    "url": "https://github.com/pit00/vscode-breakline-highlight.git",
    "type": "git"
  }
}
